<?xml version="1.0" encoding="UTF-8" ?>
<zeus2005 xmlns="http://tempuri.org/zeus2005.xsd">
  <General>
    <IS_SIMULATION>1</IS_SIMULATION>
    <USE_LUA_SCRIPTS>1</USE_LUA_SCRIPTS>
    <WRITE_LOG>1</WRITE_LOG>
    <LOG_MODE>0</LOG_MODE>
    <WRITE_RADIO_LOG>0</WRITE_RADIO_LOG><!-- output package -->
    <RECORDPOS_PORT>20007</RECORDPOS_PORT><!-- a thread to record sth in zeus.cpp-->
  </General>
  <Defence>
    <DEFENCE_DEBUG_MODE>1</DEFENCE_DEBUG_MODE><!-- defence sequence-->
    <DENY_BALL_MODE>1</DENY_BALL_MODE>
    <BACK_LINE_MODE>1</BACK_LINE_MODE>
    <SPECIAL_AREA_BACK_LINE_MODE>0</SPECIAL_AREA_BACK_LINE_MODE>

    <!-- 设置边后卫是否会盯人，1是会，0是不会 -->
    <SIDEBACK_MARKING_MODE>1</SIDEBACK_MARKING_MODE>

    <!-- 0是友好模式，1是普通模式，2是暴力模式 -->
    <MARKING_MODE>1</MARKING_MODE>

    <!-- 守门员上前补位模式 0为不补位 -->
    <GOALIE_FILL_IN_MODE>0</GOALIE_FILL_IN_MODE>

    <!-- 守门员对一传一射的响应 1直接防人 0继续跟球 -->
    <GOALIE_DEFEND_ONE_PASS_SHOOT>1</GOALIE_DEFEND_ONE_PASS_SHOOT>
  </Defence>
  <CMatchState>
    <BAYESLIST>BayesParams1</BAYESLIST>
    <BAYESPARAM>Skuba_Attack</BAYESPARAM>
  </CMatchState>
  <JAM_AND_PUSH>
    <USE_OPP_DIR>1</USE_OPP_DIR>
    <USE_DRIBBLE_TURN>0</USE_DRIBBLE_TURN>
    <USE_DRIBBLE_KICK>0</USE_DRIBBLE_KICK>
    <USE_INTERCEPT>1</USE_INTERCEPT>
    <USE_ROTATE>0</USE_ROTATE>
    <WEAK_OPP>1</WEAK_OPP>
    <GENTLE_MODE>0</GENTLE_MODE>
    <OPP_HAS_BALL_DIST>50</OPP_HAS_BALL_DIST>
    <CIRCLE_FRONT>1</CIRCLE_FRONT>
    <STATE_PRINT>0</STATE_PRINT>
  </JAM_AND_PUSH>
  <PENALTY_DEFENCE>
    <CATEGORY>3</CATEGORY>
    <FRAME>0</FRAME>
    <AHEAD>20</AHEAD>
  </PENALTY_DEFENCE>
  <CSmartGotoPositionV2>
    <VERBOSE_MODE>0</VERBOSE_MODE>
    <DRAW_TARGET>1</DRAW_TARGET>
    <DO_CHECK>1</DO_CHECK>
    <PATH_PLAN_TYPE>0</PATH_PLAN_TYPE>
  </CSmartGotoPositionV2>
  <CGotoPositionV2>
    <DSS_AVOID>1</DSS_AVOID>

    <!-- 是否对y方向进行补偿 -->
    <USE_COMPENSATION_FOR_Y>0</USE_COMPENSATION_FOR_Y>

    <!-- 守门员单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_GOALIE>300</MAX_TRANSLATION_SPEED_GOALIE>
    <MAX_TRANSLATION_ACC_GOALIE>550</MAX_TRANSLATION_ACC_GOALIE>
    <MAX_TRANSLATION_DEC_GOALIE>550</MAX_TRANSLATION_DEC_GOALIE>

    <!-- 后卫单独控制平动参数 -->
    <MAX_TRANSLATION_SPEED_BACK>300</MAX_TRANSLATION_SPEED_BACK>
    <MAX_TRANSLATION_ACC_BACK>550</MAX_TRANSLATION_ACC_BACK>
    <MAX_TRANSLATION_DEC_BACK>550</MAX_TRANSLATION_DEC_BACK>

    <!-- 其他车的平动参数 -->
    <MAX_TRANSLATION_SPEED>300</MAX_TRANSLATION_SPEED><!-- 300 -->
    <MAX_TRANSLATION_ACC>550</MAX_TRANSLATION_ACC>    <!-- 600 -->
    <MAX_TRANSLATION_DEC>550</MAX_TRANSLATION_DEC>    <!-- 600 -->

    <MAX_ROTATION_SPEED>15</MAX_ROTATION_SPEED>
    <MAX_ROTATION_ACC>15</MAX_ROTATION_ACC>
    <MAX_ROTATION_DEC>15</MAX_ROTATION_DEC>
    <TRAJECTORY_METHORD>1</TRAJECTORY_METHORD>
    <TRANSLATION_ACC_LIMIT>1400</TRANSLATION_ACC_LIMIT>
  </CGotoPositionV2>
  <CRefereeBoxInterface>
    <REFEREE_PORT>10015</REFEREE_PORT>
  </CRefereeBoxInterface>
  <CCommControl>
    <COM_NUM>3</COM_NUM>
    <COM_RATE>115200</COM_RATE>
    <PARITY>0</PARITY>
    <PVERSION>2011</PVERSION>
  </CCommControl>
  <FieldParam>
    <FRICTION>58</FRICTION>
  </FieldParam>
  <CCompensate>
    <RATIO>43</RATIO><!--(from 1 to 100)%-->
  </CCompensate>
  <BallSpeedModel>
    <TABLE_FILE>BallSpeedModel</TABLE_FILE>
  </BallSpeedModel>
  <VisionMediator>
    <RECEIVE_X_MIN>-4800</RECEIVE_X_MIN>
    <RECEIVE_X_MAX>4800</RECEIVE_X_MAX>
    <RECEIVE_Y_MIN>-3300</RECEIVE_Y_MIN>
    <RECEIVE_Y_MAX>3300</RECEIVE_Y_MAX>
    <CAMERA_0_ENABLE>1</CAMERA_0_ENABLE>
    <CAMERA_1_ENABLE>1</CAMERA_1_ENABLE>
    <CAMERA_2_ENABLE>1</CAMERA_2_ENABLE>
    <CAMERA_3_ENABLE>1</CAMERA_3_ENABLE>
    <MIN_ADD_FRAME>3</MIN_ADD_FRAME>
    <MIN_LOST_FRAME>10</MIN_LOST_FRAME>
    <SLEEP_TIME>5</SLEEP_TIME>
    <FOLLOW_CHECK_CYCLE>10</FOLLOW_CHECK_CYCLE>
    <MAX_BALL_SPEED>10</MAX_BALL_SPEED>
    <MAX_ROBOT_SPEED>4</MAX_ROBOT_SPEED>
    <CALI_ERROR>25</CALI_ERROR>
    <M_SEND>0</M_SEND>    <!--0 FOR false 1 FOR true-->
    <TRANSMIT_IP>127.0.0.1</TRANSMIT_IP>
    <TRANSMIT_PORT>54321</TRANSMIT_PORT>
    <MULTICAST_IP>224.5.23.2</MULTICAST_IP>
    <MULTICAST_PORT>10005</MULTICAST_PORT>
  </VisionMediator>
</zeus2005>
